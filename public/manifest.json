{
  "short_name": "Docto Chatbot",
  "name": "Doctolib Chat Bot",
  "version": "1.0",
  "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/doctolib_logo.png",
          "32": "images/doctolib_logo.png",
          "48": "images/doctolib_logo.png",
          "128": "images/doctolib_logo.png"
        }
  },
  "icon": {
    "16": "images/doctolib_logo.png",
    "32": "images/doctolib_logo.png",
    "48": "images/doctolib_logo.png",
    "128": "images/doctolib_logo.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "manifest_version": 3,
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
